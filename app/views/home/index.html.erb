<%= javascript_tag do -%>
  (function(root) {
      var PhotoBooth = root.PhotoBooth;

      if (!PhotoBooth) {
          console.warn("PhotoBooth settings not set!");
          return;
      }

      PhotoBooth.module("Settings", function(Settings) {
          Settings.countdownSeconds = <%= ENV["PHOTOBOOTH_COUNTDOWN_SECONDS"] %>
      });
  })(window);
<% end -%>