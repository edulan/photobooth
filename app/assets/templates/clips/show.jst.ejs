<div class="row">
  <div class="col-md-8">
    <div class="clip-group clip-medium">
      <div class="row clip-wrapper">
        <% _.each(snapshots, function(snapshot) { %>
        <div class="clip-content">
          <% if (snapshot.medium_url.indexOf('processing') !== -1) { %>
              <p class="text-muted">
                <i class="fa fa-cog fa-3x fa-spin clip-processing"></i>
              </p>
          <% } else { %>
              <img src="<%= snapshot.medium_url %>" alt="">
          <% } %>
        </div>
        <% }) %>
      </div>
    </div>
  </div>
  <div class="col-md-4">
    <div class="btn-group-vertical clip-actions">
      <a href="#" class="btn btn-default btn-like">
        <i class="fa fa-thumbs-o-up fa-1x"></i> Like (<%= votes %>)
      </a>
      <a class="btn btn-xs btn-link btn-delete" href="#">
        delete
      </a>
    </div>
  </div>
</div>
